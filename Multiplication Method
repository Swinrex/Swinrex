using System;
					
public class Program
{
	public void Main()
	{
		Console.WriteLine("Welcome to my Multiplication Program");
		Console.WriteLine("---------------------------------------------");
		Console.WriteLine(Multiply(1.2, 1.5, 3.4, 7.2));
	}
	static double Multiply(double num0, double num1, double num2, double num3)
	{
		double result = num0 * num1 * num2 * num3;
		int FinalResult = (int)result;
		return FinalResult;
	}
}
